<div class="row">
    <div class="col-md-4 col-md-offset-4 center">
    </div>
</div>
<h4><%= @user.first_name %> <%= @user.last_name %></h4>
<small>Member since <%= @user.created_at %></small>
<h4 class="center"><%= @user.username %>'s jobs</h4>
<p><button class="btn btn-primary send-message" data-toggle="modal" data-target="#message-modal">Message</button></p>

<%= render 'jobs/job', obj: @user_jobs %>

<%= render partial: "message", locals: { user: @user } %>